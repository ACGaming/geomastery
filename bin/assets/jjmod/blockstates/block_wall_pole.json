{
    "multipart": [
        {   "when": { "straight": "ns" },
            "apply": { "model": "jjmod:building/pole_straight" }
        },
        {   "when": { "straight": "ew" },
            "apply": { "model": "jjmod:building/pole_straight", "y": 90 }
        },
        {   "when": { "straight": "no" },
            "apply": { "model": "jjmod:building/pole_post" }
        },
        {   "when": { "straight": "no", "north": "true" },
            "apply": { "model": "jjmod:building/pole_side" }
        },
        {   "when": { "straight": "no", "east": "true" },
            "apply": { "model": "jjmod:building/pole_side", "y": 90 }
        },
        {   "when": { "straight": "no", "south": "true" },
            "apply": { "model": "jjmod:building/pole_side", "y": 180 }
        },
        {   "when": { "straight": "no", "west": "true" },
            "apply": { "model": "jjmod:building/pole_side", "y": 270 }
        }
    ]
}