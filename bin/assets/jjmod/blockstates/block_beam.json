{
    "multipart": [
        {   
            "when": { "axis": "ew" },
            "apply": { "model": "jjmod:building/beam" }
        },
        {
            "when": { "axis": "ns" },
            "apply": { "model": "jjmod:building/beam", "y": 90 }
        },
        {
            "when": { "northend": "true" },
            "apply": { "model": "jjmod:building/beam_end", "y": 270 }
        },
        {
            "when": { "eastend": "true" },
            "apply": { "model": "jjmod:building/beam_end" }
        },
        {
            "when": { "southend": "true" },
            "apply": { "model": "jjmod:building/beam_end", "y": 90 }
        },
        {
            "when": { "westend": "true" },
            "apply": { "model": "jjmod:building/beam_end", "y": 180 }
        },
        {
            "when": { "floor": "pole" },
            "apply": { "model": "jjmod:building/floor_pole", "y": 90 }
        },
        {
            "when": { "floor": "wood" },
            "apply": { "model": "jjmod:building/floor_wood", "y": 90 }
        },
        {
            "when": { "floor": "pole", "northside": "true" },
            "apply": { "model": "jjmod:building/floor_pole_side", "y": 90 }
        },
        {
            "when": { "floor": "pole", "eastside": "true" },
            "apply": { "model": "jjmod:building/floor_pole_side", "y": 180 }
        },
                {
            "when": { "floor": "pole", "southside": "true" },
            "apply": { "model": "jjmod:building/floor_pole_side", "y": 270 }
        },
        {
            "when": { "floor": "pole", "westside": "true" },
            "apply": { "model": "jjmod:building/floor_pole_side" }
        },
        {
            "when": { "floor": "wood", "northside": "true" },
            "apply": { "model": "jjmod:building/floor_wood_side", "y": 90 }
        },
        {
            "when": { "floor": "wood", "eastside": "true" },
            "apply": { "model": "jjmod:building/floor_wood_side", "y": 180 }
        },
                {
            "when": { "floor": "wood", "southend": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end", "y": 270 }
        },
        {
            "when": { "floor": "wood", "westend": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end" }
        },
                {
            "when": { "floor": "pole", "northend": "true" },
            "apply": { "model": "jjmod:building/floor_pole_end", "y": 90 }
        },
        {
            "when": { "floor": "pole", "eastend": "true" },
            "apply": { "model": "jjmod:building/floor_pole_end", "y": 180 }
        },
                {
            "when": { "floor": "pole", "southend": "true" },
            "apply": { "model": "jjmod:building/floor_pole_end", "y": 270 }
        },
        {
            "when": { "floor": "pole", "westend": "true" },
            "apply": { "model": "jjmod:building/floor_pole_end"}
        },
        {
            "when": { "floor": "wood", "northend": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end", "y": 90 }
        },
        {
            "when": { "floor": "wood", "eastend": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end", "y": 180 }
        },
                {
            "when": { "floor": "wood", "southend": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end", "y": 270 }
        },
        {
            "when": { "floor": "wood", "westend": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end" }
        },
        {
            "when": { "floor": "pole", "northend": "true", "eastside": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner", "y": 90 }
        },
        {
            "when": { "floor": "pole", "northside": "true", "eastend": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner", "y": 90 }
        },
        {
            "when": { "floor": "pole", "eastend": "true", "southside": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner", "y": 180 }
        },
        {
            "when": { "floor": "pole", "eastside": "true", "southend": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner", "y": 180 }
        },
        {
            "when": { "floor": "pole", "southend": "true", "westside": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner", "y": 270 }
        },
        {
            "when": { "floor": "pole", "southside": "true", "westend": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner", "y": 270 }
        },
        {
            "when": { "floor": "pole", "westend": "true", "northside": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner" }
        },
        {
            "when": { "floor": "pole", "westside": "true", "northend": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner" }
        }
    ]
}
