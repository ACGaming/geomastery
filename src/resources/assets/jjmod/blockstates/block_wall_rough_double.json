{
    "multipart": [
        {   "when": { "straight": "ns" },
            "apply": { "model": "jjmod:wall_rough_double/straight" }
        },
        {   "when": { "straight": "ew" },
            "apply": { "model": "jjmod:wall_rough_double/straight", "y": 90 }
        },
        {   "when": { "straight": "no" },
            "apply": { "model": "jjmod:wall_rough_double/post" }
        },
        {   "when": { "straight": "no", "north": "true" },
            "apply": { "model": "jjmod:wall_rough_double/side", "y": 180 }
        },
        {   "when": { "straight": "no", "east": "true" },
            "apply": { "model": "jjmod:wall_rough_double/side", "y": 270 }
        },
        {   "when": { "straight": "no", "south": "true" },
            "apply": { "model": "jjmod:wall_rough_double/side" }
        },
        {   "when": { "straight": "no", "west": "true" },
            "apply": { "model": "jjmod:wall_rough_double/side", "y": 90 }
        }
    ]
}