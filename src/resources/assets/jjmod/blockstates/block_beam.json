{
    "multipart": [
        {   
            "when": { "axis": "ew" },
            "apply": { "model": "jjmod:building/beam" }
        },
        {
            "when": { "axis": "ns" },
            "apply": { "model": "jjmod:building/beam", "y": 90 }
        },
        {
            "when": { "axis": "ns", "front": "true" },
            "apply": { "model": "jjmod:building/beam_end", "y": 270 }
        },
        {
            "when": { "axis": "ns", "back": "true" },
            "apply": { "model": "jjmod:building/beam_end", "y": 90 }
        },
        {
            "when": { "axis": "ew", "front": "true" },
            "apply": { "model": "jjmod:building/beam_end" }
        },
        {
            "when": { "axis": "ew", "back": "true" },
            "apply": { "model": "jjmod:building/beam_end", "y": 180 }
        },
        {
            "when": { "axis": "ns", "floor": "pole" },
            "apply": { "model": "jjmod:building/floor_pole", "y": 90 }
        },
        {
            "when": { "axis": "ew", "floor": "pole" },
            "apply": { "model": "jjmod:building/floor_pole" }
        },
        {
            "when": { "axis": "ns", "floor": "wood" },
            "apply": { "model": "jjmod:building/floor_wood", "y": 90 }
        },
        {
            "when": { "axis": "ew", "floor": "wood" },
            "apply": { "model": "jjmod:building/floor_wood" }
        },
        {
            "when": { "axis": "ns", "floor": "pole", "front": "true" },
            "apply": { "model": "jjmod:building/floor_pole_end", "y": 90 }
        },
        {
            "when": { "axis": "ns", "floor": "pole", "back": "true" },
            "apply": { "model": "jjmod:building/floor_pole_end", "y": 270 }
        },
        {
            "when": { "axis": "ew", "floor": "pole", "front": "true" },
            "apply": { "model": "jjmod:building/floor_pole_end", "y": 180 }
        },
        {
            "when": { "axis": "ew", "floor": "pole", "back": "true" },
            "apply": { "model": "jjmod:building/floor_pole_end" }
        },
        {
            "when": { "axis": "ns", "floor": "wood", "front": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end", "y": 90 }
        },
        {
            "when": { "axis": "ns", "floor": "wood", "back": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end", "y": 270 }
        },
        {
            "when": { "axis": "ew", "floor": "wood", "front": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end", "y": 180 }
        },
        {
            "when": { "axis": "ew", "floor": "wood", "back": "true" },
            "apply": { "model": "jjmod:building/floor_wood_end" }
        },
        {
            "when": { "axis": "ns", "floor": "pole", "left": "true" },
            "apply": { "model": "jjmod:building/floor_pole_right", "y": 90 }
        },
        {
            "when": { "axis": "ns", "floor": "pole", "right": "true" },
            "apply": { "model": "jjmod:building/floor_pole_left", "y": 90 }
        },
        {
            "when": { "axis": "ew", "floor": "pole", "left": "true" },
            "apply": { "model": "jjmod:building/floor_pole_left" }
        },
        {
            "when": { "axis": "ew", "floor": "pole", "right": "true" },
            "apply": { "model": "jjmod:building/floor_pole_right" }
        },
        {
            "when": { "axis": "ns", "floor": "wood", "left": "true" },
            "apply": { "model": "jjmod:building/floor_wood_left", "y": 90 }
        },
        {
            "when": { "axis": "ns", "floor": "wood", "right": "true" },
            "apply": { "model": "jjmod:building/floor_wood_right", "y": 90 }
        },
        {
            "when": { "axis": "ew", "floor": "wood", "left": "true" },
            "apply": { "model": "jjmod:building/floor_wood_right" }
        },
        {
            "when": { "axis": "ew", "floor": "wood", "right": "true" },
            "apply": { "model": "jjmod:building/floor_wood_left" }
        },
        {
            "when": { "axis": "ns", "floor": "pole", "fl": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner_right", "y": 90 }
        },
        {
            "when": { "axis": "ns", "floor": "pole", "fr": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner_left", "y": 90 }
        },
        {
            "when": { "axis": "ns", "floor": "pole", "bl": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner_left", "y": 270 }
        },
        {
            "when": { "axis": "ns", "floor": "pole", "br": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner_right", "y": 270 }
        },
        {
            "when": { "axis": "ew", "floor": "pole", "fl": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner_right", "y": 180 }
        },
        {
            "when": { "axis": "ew", "floor": "pole", "fr": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner_left", "y": 180 }
        },
        {
            "when": { "axis": "ew", "floor": "pole", "bl": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner_left" }
        },
        {
            "when": { "axis": "ew", "floor": "pole", "br": "true" },
            "apply": { "model": "jjmod:building/floor_pole_corner_right" }
        },
        {
            "when": { "axis": "ns", "floor": "wood", "fl": "true" },
            "apply": { "model": "jjmod:building/floor_wood_corner_right", "y": 90 }
        },
        {
            "when": { "axis": "ns", "floor": "wood", "fr": "true" },
            "apply": { "model": "jjmod:building/floor_wood_corner_left", "y": 90 }
        },
        {
            "when": { "axis": "ns", "floor": "wood", "bl": "true" },
            "apply": { "model": "jjmod:building/floor_wood_corner_left", "y": 270 }
        },
        {
            "when": { "axis": "ns", "floor": "wood", "br": "true" },
            "apply": { "model": "jjmod:building/floor_wood_corner_right", "y": 270 }
        },
        {
            "when": { "axis": "ew", "floor": "wood", "fl": "true" },
            "apply": { "model": "jjmod:building/floor_wood_corner_right", "y": 180 }
        },
        {
            "when": { "axis": "ew", "floor": "wood", "fr": "true" },
            "apply": { "model": "jjmod:building/floor_wood_corner_left", "y": 180 }
        },
        {
            "when": { "axis": "ew", "floor": "wood", "bl": "true" },
            "apply": { "model": "jjmod:building/floor_wood_corner_left" }
        },
        {
            "when": { "axis": "ew", "floor": "wood", "br": "true" },
            "apply": { "model": "jjmod:building/floor_wood_corner_right" }
        }
    ]
}
